<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.taobook.datasour.OrderlistEntity" table="ORDERLIST" schema="BOOK_SELLING">
        <id name="id" column="id"/>
        <property name="count" column="count"/>
        <property name="pay" column="pay"/>
        <property name="payType" column="pay_type"/>
        <property name="status" column="status"/>
        <property name="subTime" column="sub_time"/>
        <property name="accTime" column="acc_time"/>
        <property name="sedTime" column="sed_time"/>
        <property name="recTime" column="rec_time"/>
        <property name="comment" column="comment"/>
        <property name="type" column="type"/>
        <many-to-one name="item" class="com.example.taobook.datasour.ItemEntity" column="item_id"/>
        <many-to-one name="account" class="com.example.taobook.datasour.AccountEntity" column="buy_acid"/>
    </class>
</hibernate-mapping>