<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.taobook.datasour.AccountEntity" table="ACCOUNT" schema="BOOK_SELLING">
        <id name="id" column="id"/>
        <property name="status" column="status"/>
        <property name="balance" column="balance"/>
        <property name="isBank" column="is_bank"/>
        <property name="bankType" column="bank_type"/>
        <property name="bankAccount" column="bank_account"/>
    </class>
</hibernate-mapping>